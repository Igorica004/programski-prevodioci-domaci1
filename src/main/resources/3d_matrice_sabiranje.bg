begin function add3D(aint[][][], bint[][][], nint, mint, pint)int[][][]
  int[i][j][k] c
  int i,j,k
  begin for (i goes from 0 to n-1)
    begin for (j goes from 0 to m-1)
      begin for (k goes from 0 to p-1)
        a[i][j][k]  b[i][j][k]  c[i][j][k]
      end for
    end for
  end for
  return c
end function

int[2][2][2] a,b,c


