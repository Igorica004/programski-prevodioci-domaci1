"tes t" 't' -0.4
procedura bubbleSort(podatak[] a, podatak n)
{
  podatak       i,    j,tmp,n1,n2;
  n1 podesi n-1;
  ponovi(i podesi 0; i < n1; 1)
  {
    n2 podesi n-i-2;
    ponovi(j podesi 0; j < n2; 1)
    {
      proveri(a[j] > a[j+1]) pa
      {
        tmp podesi a[j];
        a[j] podesi a[j+1];
        a[j+1] podesi tmp;
      }
    }
  }
}

podatak[5] arr;
arr[0] podesi 4;
arr[1] podesi 2;
arr[2] podesi 5;
arr[3] podesi 1;
arr[4] podesi 3;

bubbleSort(arr,5);
print1D(arr, 5);